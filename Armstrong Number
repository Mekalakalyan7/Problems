import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int sum = 0;
		int dup = n;
		int ld = 0;
		while(n>0)
		{
             ld = n%10;
			 sum = sum+(ld*ld*ld);
			 n /=10;

		}
		if(sum==dup)
		{
			System.out.println("true");
		}
		else
		{
			System.out.println("false");
		}

	}
}



