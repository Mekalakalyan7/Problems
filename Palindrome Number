import java.util.Scanner;
public class Solution {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int dup = n;
		int rev = 0;
		int ld = 0;
		while(n>0)
		{
			ld = n%10;
			rev = (rev*10)+ld;
			n /=10;

		}
		if(rev==dup)
		{
			System.out.println("true");
		}
		else
		{
             System.out.println("false");
		}
		
	}
}
